<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <%- css('/css/main.css') %>
</head>
<body>
<div class="content-wrapper bg-background-primary font-sans text-copy-primary leading-normal flex flex-col min-h-screen">
    <header class="border-t-14 border-blue-700">
        <nav class="container mx-auto flex flex-wrap justify-between items-center py-8">
            <div>
                <a href="<%- url_for() %>">
                    <%- image_tag('/img/logo-100.png', {class: "inline w-12", alt: "Coding with Johan"}) %>
                    <p class="inline text-copy-primary text-xl ml-4">Coding with Johan</p>
                </a>
            </div>
            <div class="block lg:hidden">
                <button class="flex items-center px-3 py-2 border rounded border-gray-500 hover:text-gray-600 hover:border-gray-600">
                    <svg class="current-color h-3 w-3" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                        <path d="M0 3h20v2H0V3zm0 6h20v2H0V9zm0 6h20v2H0v-2z" fill="gray"/>
                    </svg>
                </button>
            </div>
            <ul class="top-menu uppercase tracking-wide font-bold w-full block flex-grow lg:space-x-8 space-y-6 lg:space-y-0 lg:flex lg:flex-initial lg:w-auto items-center mt-8 lg:mt-0">
                <li class="mb-6 lg:mb-0">
                    <!-- TODO: Search box -->
                </li>
                <li>
                    <!-- TODO: Theme toggle -->
                </li>
                <li>
                    <a href="<%- url_for('blog') %>" class="text-copy-primary hover:text-gray-600">Blog</a>
                </li>
            </ul>
        </nav>
    </header>

    <div class="flex-grow">
        <%- body %>
    </div>
    <footer class="bg-blue-700 text-white">
        <div class="container mx-auto flex flex-col lg:flex-row items-center justify-between py-8">
            <div class="mb-8 lg:mb-0">
                <div>Copyright <%- new Date().getFullYear() %>. All rights reserved.</div>
            </div>
            <ul class="flex items-center space-x-8">
                <li>
                    <a href="mailto:johanvergeer@gmail.com" class="text-white hover:text-gray-400">
                        <%- image_tag('/img/icons/email.png', {alt: "email"}) %>
                    </a>
                </li>

                <li>
                    <a href="https://www.youtube.com/channel/UC9YXmkTSfE7A2lCs-ndrmxw" target="_blank"
                       class="text-white hover:text-gray-400">
                        <%- image_tag('/img/icons/youtube.png', {alt: "youtube"}) %>
                    </a>
                </li>

                <li>
                    <a href="https://github.com/johanvergeer" target="_blank"
                       class="text-white hover:text-gray-400">
                        <%- image_tag('/img/icons/github.png', {alt: "github"}) %>
                    </a>
                </li>

                <li>
                    <a href="https://twitter.com/johan_vergeer" target="_blank" class="text-white hover:text-gray-400">
                        <%- image_tag('/img/icons/twitter.png', {alt: "twitter"}) %>
                    </a>
                </li>

                <li>
                    <a href="https://linkedin.com/in/johanvergeer" target="_blank"
                       class="text-white hover:text-gray-400">
                        <%- image_tag('/img/icons/linkedin.png', {alt: "linkedin"}) %>
                    </a>
                </li>
            </ul>
        </div>
    </footer>

